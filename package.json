{
  "name": "n8n-nodes-websocket",
  "version": "1.1.3",
  "description": "n8n WebSocket nodes with workflow-based paths",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-package",
    "websocket"
  ],
  "license": "MIT",
  "homepage": "https://github.com/subinksoman/n8n-nodes-websocket",
  "author": {
    "name": "Subin Soman",
    "email": "subinksoman@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subinksoman/n8n-nodes-websocket.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist/",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*",
    "credentials/**/*",
    "nodes/**/*",
    "*.svg",
    "*.png"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/WebSocketTrigger/WebSocketTrigger.node.js",
      "dist/nodes/WebSocketResponse/WebSocketResponse.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.16",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "n8n-workflow": "~1.5.0",
    "ws": "^8.13.0"
  }
}
